{
  "name": "qmesh",
  "version": "0.1.0",
  "description": "Distributed peer-to-peer LLM inference network",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "worker": "node examples/run-worker.js",
    "client": "node examples/basic-client.js",
    "download": "node scripts/download-model.js",
    "test": "node --experimental-test-module-mocks --test tests/**/*.test.js",
    "test:unit": "node --test tests/unit/**/*.test.js",
    "test:integration": "node --test tests/integration/**/*.test.js",
    "test:inference": "node examples/simple-inference.js",
    "lint": "echo 'Linting not configured yet'",
    "format": "echo 'Formatting not configured yet'",
    "dev": "node --watch examples/run-worker.js"
  },
  "keywords": [
    "llm",
    "inference",
    "p2p",
    "distributed",
    "ai",
    "machine-learning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-llama-cpp": "^3.1.1",
    "@inquirer/prompts": "^7.0.1",
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.1"
  }
}
